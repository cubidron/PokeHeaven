{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "PhynariaMC",
  "version": "0.1.0",
  "identifier": "com.cubidron.Phynaria.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "PhynariaMC",
        "width": 1024,
        "height": 768,
        "minWidth": 1024,
        "minHeight": 768,
        "resizable": true,
        "transparent": true,
        "decorations": false,
        "windowEffects": {
          "effects": [],
          "radius": 10
        }
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "sql": {
      "preload": [
        "sqlite:data.db"
      ]
    }
  }
}